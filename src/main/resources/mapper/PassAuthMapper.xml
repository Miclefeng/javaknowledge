<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="PassAuthMapper">
    <select id="selectAll" resultType="com.javase.base.mybatis.entity.PassAuth">
        SELECT * FROM `pass_auth` WHERE `is_delete`=0 ORDER BY `id` DESC LIMIT 10;
    </select>
</mapper>
